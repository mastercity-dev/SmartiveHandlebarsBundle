{
  "name": "smartive/handlebars-bundle",
  "description": "This bundle integrates rendering of Handlebars templates into Symfony2",
  "license": "MIT",
  "license": "proprietary",
  "authors": [
    {
      "name": "smartive GmbH",
      "homepage": "https://www.smartive.ch"
    }
  ],
  "support": {
    "source": "https://github.com/smartive/SmartiveHandlebarsBundle"
  },
  "autoload": {
    "psr-4": { "Migros\\ComponentsBundle\\": "" }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:migros/MigrosModels.git"
    },
    {
      "type": "vcs",
      "url": "git@github.com:migros/MigrosSdkBundle.git"
    }
  ],
  "require": {
    "php": ">=5.5.0",
    "symfony/framework-bundle": "~2.6",
    "symfony/finder": "~2.6",
    "xamin/handlebars.php": "~0.10"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "~2.0",
    "phpunit/phpunit": "~4.0"
  },
  "config": {
    "bin-dir": "bin",
    "process-timeout": 1800
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.x-dev"
    }
  }
}
